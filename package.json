{
  "name": "leetcode-maxxing",
  "displayName": "Leetcode Maxxing",
  "version": "0.0.3",
  "description": "An extension to help you prepare for DSA",
  "type": "module",
  "author": "JaveRynx",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.10.0",
    "plasmo": "0.89.1",
    "puppeteer": "^22.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "declarativeNetRequest",
      "declarativeNetRequestFeedback",
      "storage",
      "alarms",
      "tabs",
      "webRequest"
    ],
    "web_accessible_resources": [
      {
        "resources": [],
        "matches": [
          "https://www.plasmo.com/*"
        ]
      }
    ],
    "host_permissions": [
      "https://*/*"
    ]
  }
}